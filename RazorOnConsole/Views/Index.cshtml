@using System.Linq

@{
    var foo = "bar" + "foo";
    var link = "http://tugberkugurlu.com";
}

@functions
{
    public Index(string model)
    {
        Model = model;
    }

    public string Model { get; set; }
}

<a href="@link">@foo</a>

@foreach (var i in Enumerable.Range(0, 10))
{
    @: <a href="#@(i)">@(i)</a>
}

<p>This is model: @Model..</p>